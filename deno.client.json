{
  "fmt": {
    "proseWrap": "never",
    "singleQuote": true,
    "semiColons": false,
    "exclude": ["www/index.js"]
  },
  "lint": {
    "exclude": ["www/index.js"]
  },
  "test": {
    "exclude": ["www/index.js"]
  },
  "imports": {
    "std/": "https://deno.land/std@0.200.0/",
    "solid-js": "npm:solid-js",
    "solid-js/web": "npm:solid-js/web",
    "solidjs-use": "npm:solidjs-use",
    "motion": "npm:motion",
    "motion/solid": "npm:@motionone/solid"
  },
  "compilerOptions": {
    "lib": ["esnext", "dom", "deno.window"],
    "jsx": "react-jsx",
    "jsxImportSource": "npm:solid-js"
  }
}
